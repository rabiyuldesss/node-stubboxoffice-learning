<style>
    * {
        margin: 0;
        padding: 0;
    }

    ul {
        list-style: none;
    }

    .clearfix:after,
    .clearfix:before {
        content: "";
        clear: both;
    }

    .clearfix:after {
        display: table;
    }

    /* Style the tab */
    .tab {
        float: left;
        border-bottom: 1px solid #ccc;
        width: 65%;
        background-color: rgba(0,0,0,0.7);
    }
.nav-bg-enabled .tab,
.nav-bg-enabled .tabcontent{
    width: 100%;
}
    /* Style the buttons inside the tab */
    .tab button {
        display: block;
        color: #fff;
        padding: 10px 20px !important;
        border-bottom: 3px solid transparent;
        outline: none;
        text-align: left;
        cursor: pointer !important;
        font-size: 15px !important;
        border-radius: 0 !important;
        float: left;
        background-color: transparent;
        box-shadow: none;
    }

    /* Change background color of buttons on hover */
    .tab button:hover {
        border-bottom: 3px solid #dc4041;
        color: #dc4041 !important;
    }

    /* Create an active/current "tab button" class */
    .tab button.active {
        border-bottom: 3px solid #dc4041 !important;
        color: #dc4041;
    }

    /* Style the tab content */
    .tabcontent {
        float: left;
        padding: 0px 12px;
        width: 65%;
        border-left: none;
        display: none;
        background-color: rgba(0,0,0,0.7);
        max-height: 500px;
        min-height: 250px;
        overflow-y: auto;

    }

    .tabcontent .list-horizon {
        width: 33%;
        float: left;
    }

    .full-width-list {
        width: 100% !important;
        border-top: 1px solid #ccc;
    }

    .full-width-list a {
        padding: 10px;
        display: block;
        margin: 0px;
        font-size: 17px !important;
        font-weight: 600 !important;
        color: #dc4041 !important;
    }

    /* .menu-container .manu ul li ul ul:first-of-type {
	display: block !important;
  } */
    /*=================================*/
    .menu-container {
        width: 100%;
        display: none;
        z-index: 100;
        margin: 0 auto 0 auto;
        /* background-image: linear-gradient(rgba(0,0,0,.4),rgba(0,0,0,.2),rgba(0,0,0,.0)) */
    }

    .menu-container:after,
    .menu-container:before {
        content: "";
        clear: both;
    }

    .menu-container:after {
        display: table;
    }

    .menu-container .manu {
        width: 95%;
        display: block;
        position: relative;
        margin: 0 auto 0 auto;
    }

    .menu ul:before,
    .menu ul:after {
        content: "";
        display: table;
    }

    .menu ul:after {
        clear: both;
    }

    .menu:before,
    .menu:after {
        content: "";
        display: table;
    }

    .menu:after {
        clear: both;
    }

    .menu ul li ul:before,
    .menu ul li ul:after {
        content: "";
        display: table;
    }

    .menu ul li ul:after {
        clear: both;
    }
    .list-horizon li {
        float: none !important;
        /* background-color: transparent; */
    }
    .menu-container .manu ul li {
        float: left;
        background-color: transparent;
    }

    .menu-container .manu ul li a {
        display: block;
        padding: 15px 25px;
        color: #fff;
        font-size: 16px;
        transition: .2s;
    }

    .menu-container .manu ul li ul li a {
        display: block;
        padding: 8px;
        color: #fff;
        font-size: 13px;
        transition: .2s;
    }

    .menu-container .manu ul li ul li:hover {
        background-color: #000;
    }

    .menu-container .manu ul li a span {
        transition: .3s all ease-out;
        line-height: 0px;
    }

    .menu-container .manu ul li a:hover span {
        transform: rotate(180deg);
        padding-right: 5px;
    }

    span.drop_down_icon {
        color: #fff;
        padding-left: 5px;
        font-size: 18px;
        position: absolute;
        margin-top: 11px;
    }

    .menu-container .manu ul li .have_dropdown {
        background-color: transparent;
        position: absolute;
        /*	left: 0;*/
        display: none;
        width: 100%;
    }

    .menu-container .manu ul li .have_dropdown_n_style {
        background-color: transparent;
        position: absolute;
        left: 0;
        display: none;
        width: 100%;
    }

    .menu-container .manu ul li .have_dropdown_n_style li a {
        color: #dc4041;
        border-bottom: 1px solid #fff;
    }

    .menu-container .manu ul li .have_dropdown_n_style li ul li a {
        color: #252525;
        border-bottom: none;
    }

    .menu-container .manu ul li .have_dropdown_n_style li ul li a:hover {
        color: #dc4041;
        font-weight: normal;
        background-color: #fafafa;
        text-decoration: underline;
    }

    .menu-container .manu ul li .have_dropdown_n_style li {
        width: 25%;
        float: left;
    }

    .menu-container .manu ul li .have_dropdown_n_style {
        background-color: #fff;
    }

    .menu-container .manu ul li .have_dropdown li a {
        /* float: left; */
        color: #fff;
        /* background-color: #fff; */
        font-weight: normal;
    }

    .menu-container .manu ul li .have_dropdown li ul li {
        display: none;
    }

    .menu-container .manu ul li .have_dropdown_n_style li ul li {
        background-color: #fff;
        height: auto;
        float: none;
        display: block;
        width: 100%;
    }

    .nav_menu_toggler_icon {
        cursor: pointer;
        position: absolute;
        right: 0;
        top: 0%;
        background: none;
        color: #fff;
        font-size: 30px !important;
        z-index: 150;
        display: none;
        border: none;
        background-color: transparent !important;
    }

    .nav_menu_toggler_icon span {
        padding: 2px 7px;
        border-radius: 8px;
    }

    .nav_menu_toggler_icon:active {
        color: #ddd;
    }

    .container_help {
        text-align: center;
        margin-top: 150px;
    }

    .container_help h2,
    .container_help h1 {
        color: #fff;
        font-size: 30px;
        font-weight: normal;
    }

    .mobile-menu {
        display: none;
    }
    nav.nav-bg-enabled #main-nav .inner .content #nav-search{
        display: block !important;
        background-color:#000;
        padding-top: 15px;
    }

    @media only screen and (max-width: 756px) {
        .menu-container .manu ul li .have_dropdown li ul li {
            display: block !important;
        }

        .desktop-menu {
            display: none;
        }

        .mobile-menu {
            display: block;
        }

        .tab,
        .tab button,
        .tabcontent {
            width: 100%;
        }

        .nav_menu_toggler_icon {
            display: block;
        }

        .mobile-btns {
            position: absolute;
            right: 20%;
            top: 25%;
        }

        .menu-container .manu {
            display: none;
            position: absolute;
            width: 100%;
            left: 0;
            background-color: #252525;
            top: 70%;
        }

        .menu-container .manu ul li {
            float: none;
            display: block;
        }

        .menu-container .manu ul li .have_dropdown {
            background-color: transparent;
            display: none;
            position: relative;
            max-height: 280px;
            min-height: 240px;
            overflow-y: auto;
        }

        .menu-container .manu ul li .have_dropdown li a {
            color: #dc4041;
            font-weight: 600;
        }

        .menu-container .manu ul li .have_dropdown li ul li a {
            color: #252525;
            font-weight: normal !important;
        }

        .menu-container .manu ul li .have_dropdown li {
            display: block;
            float: none;
            width: 100%;
            background-color: #fff !important;
        }

        .menu-container .manu ul li .have_dropdown li a {
            display: block;
            float: none;
            width: 100%;
            padding: 5px 40px !important;
            font-size: 13px;
        }

        .menu-container .manu ul li .have_dropdown_n_style li {
            width: 100%;
            float: none;
        }

        .menu-container .manu ul li .have_dropdown_n_style li a {
            float: none;
        }

        .menu-container .manu ul li .have_dropdown_n_style {
            position: relative;
        }
    }
    .font-weight-bold{
        font-weight: 600 !important;
    font-size: 14px !important;
    padding: 7px 8px !important;
    }
</style>


