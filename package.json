{
  "name": "stubbox",
  "version": "1.0.0",
  "description": "Stub BoxOffice, a resale ticketing site",
  "main": "app.js",
  "scripts": {
    "production": "npm run production:admin && npm run production:start",
    "production:admin": "cd admin/ && pm2 start ./bin/www -n Admin",
    "production:start": "pm2 start ecosystem.config.js",
    "production:stop": "pm2 delete all",
    "production:restart": "npm run production:stop && npm run production:start",
    "dev": "npm-run-all -p start-dev:*",
    "start-dev:admin": "cd admin/ && nodemon ./bin/www",
    "start-dev:scss": "npm-run-all -p scss:*",
    "start-dev:server": "nodemon app.js",
    "start-admin": "nodemon ../StubAdmin/bin/www",
    "scss:watch": "chokidar 'assets/src/scss/**/*.scss' -c 'npm run scss:build'",
    "scss:build": "npm-run-all -p build-task:*",
    "build-task:scss-compile": "node-sass --source-map true assets/src/scss/ -o assets/dist/css",
    "build-task:autoprefixer": "postcss assets/dist/css/*.css --use autoprefixer -d assets/dist/css"
  },
  "author": "two words LLC",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "bot-detector": "^1.0.10",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "csv-parser": "^2.3.0",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "easy-soap-request": "^2.4.1",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-sanitizer": "^1.0.5",
    "helmet": "^3.16.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "pm2": "^3.5.0",
    "rapidx2j": "^0.5.0",
    "redis": "^2.8.0",
    "slug": "^1.0.0",
    "winston": "^3.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "chokidar-cli": "^1.2.2",
    "node-sass-chokidar": "^1.3.4",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  }
}
